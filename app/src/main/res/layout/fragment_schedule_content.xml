<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/schedule_listview"
        android:layout_alignParentStart="true"
        android:layout_centerInParent="true"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TeamList"
        android:id="@+id/TeamListbutton"
        android:onClick="onClickTeamList"
        android:layout_below="@+id/schedule_listview"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="boooo"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/TeamListbutton"
        android:layout_toEndOf="@+id/TeamListbutton" />


</RelativeLayout>